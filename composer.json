{
    "name": "ypho/scryfall",
	"homepage": "https://ypho.nl",
	"keywords": ["scryfall", "scryfall.com", "api wrapper", "magic the gathering", "magic"],
    "description": "A wrapper around the API from Scryfall (scryfall.com)",
    "license": "Apache-2.0",
    "require": {
	    "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Ypho\\Scryfall\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ypho\\Scryfall\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Ivo Geersen",
            "email": "ivo@ypho.nl"
        }
    ]
}
